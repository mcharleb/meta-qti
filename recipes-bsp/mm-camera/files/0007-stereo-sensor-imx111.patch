+#ifdef CAMERA_CONF_3D
+ /*===========================================================================
+ * FUNCTION    - qs_imx111_adjust 3d_gain -
+ *
+ * DESCRIPTION:
+ *==========================================================================*/
+int32_t qs_imx111_adjust_3d_gain(void *sctrl,
+  uint16_t gain, uint32_t line)
+{
+  struct sensor_cfg_data cfg;
+  sensor_ctrl_t *ctrl = (sensor_ctrl_t *) sctrl;
+
+  if (ctrl->sfd <= 0)
+  return FALSE;
+
+  if (ctrl->sensor.op_mode == SENSOR_MODE_PREVIEW
+      || ctrl->sensor.op_mode == SENSOR_MODE_VIDEO
+      || ctrl->sensor.op_mode == SENSOR_MODE_ZSL) {
+    if (ctrl->sensor.current_gain == gain &&
+      ctrl->sensor.current_linecount == line)
+      return TRUE;
+
+    cfg.cfgtype = CFG_SET_EXP_GAIN;
+    cfg.mode = SENSOR_PREVIEW_MODE;
+  } else if (ctrl->sensor.op_mode == SENSOR_MODE_SNAPSHOT) {
+    cfg.mode = SENSOR_SNAPSHOT_MODE;
+    cfg.cfgtype = CFG_SET_PICT_EXP_GAIN;
+  } else if (ctrl->sensor.op_mode == SENSOR_MODE_RAW_SNAPSHOT) {
+    cfg.mode = SENSOR_RAW_SNAPSHOT_MODE;
+    cfg.cfgtype = CFG_SET_PICT_EXP_GAIN;
+  }
+
+  cfg.cfg.sensor_3d_exp.gain = gain;
+  cfg.cfg.sensor_3d_exp.line = line;
+  if((ctrl->sensor.cam_mode == CAMERA_MODE_3D) && (cali_data_status == 1)) {
+    float gain_3d;
+    float r_gain, b_gain, gb_gain;
+
+    gain_3d = qs_imx111_register_to_real_gain(gain);
+    r_gain = (gain_3d * qs_imx111_delta.r_delta);
+    b_gain  = (gain_3d * qs_imx111_delta.b_delta);
+    gb_gain = (gain_3d * qs_imx111_delta.gb_delta);
+    cfg.cfg.sensor_3d_exp.r_gain = qs_imx111_real_to_register_gain(r_gain);
+    cfg.cfg.sensor_3d_exp.b_gain = qs_imx111_real_to_register_gain(b_gain);
+    cfg.cfg.sensor_3d_exp.gb_gain = gain;
+    cfg.cfg.sensor_3d_exp.gr_gain = gain;
+  }
+
+  if (ioctl(ctrl->sfd, MSM_CAM_IOCTL_SENSOR_IO_CFG, &cfg) < 0) {
+    CDBG("qs_imx111 failed %d\n", __LINE__);
+    return FALSE;
+  }
+  ctrl->sensor.current_gain = gain;
+  ctrl->sensor.current_linecount = line;
+  return TRUE;
+}
+/*===========================================================================
+ * FUNCTION    - x -
+ *
+ * DESCRIPTION: IMX111 Process Calibration
+ *==========================================================================*/
+int8_t qs_imx111_get_3d_calib_data(void *sctrl)
+{
+  int i, j;
+  sensor_ctrl_t *ctrl = (sensor_ctrl_t *) sctrl;
+
+  qs_imx111_delta.r_delta = ((float)qs_imx111_whitebalance_calib_data[0]/(float)Q7) \
+         / ((float)qs_imx111_whitebalance_calib_data[1]/(float)Q7);
+
+  qs_imx111_delta.b_delta = ((float)qs_imx111_whitebalance_calib_data[2]/(float)Q7) \
+         / ((float)qs_imx111_whitebalance_calib_data[3]/(float)Q7);
+
+  qs_imx111_delta.gb_delta = ((float)qs_imx111_whitebalance_calib_data[4]/(float)Q7) \
+         / ((float)qs_imx111_whitebalance_calib_data[5]/(float)Q7);
+
+  for (i=0; i<3; i++) {
+    for (j=0; j<4; j++) {
+      ctrl->cali_data_3d->left_p_matrix[i][j] = \
+        *((double *)&qs_imx111_3d_calib_left_data[i][j][0]);
+      ctrl->cali_data_3d->right_p_matrix[i][j] = \
+        *((double *)&qs_imx111_3d_calib_right_data[i][j][0]);
+    }
+  }
+
+  ctrl->cali_data_3d->square_len = *((double*) qs_imx111_3d_calib_square_len);
+  ctrl->cali_data_3d->focal_len = *((double*) qs_imx111_3d_calib_focal_len);
+  ctrl->cali_data_3d->pixel_pitch = *((double*) qs_imx111_3d_calib_pixel_patch);
+  cali_data_status = 1;
+
+  CDBG("qs_imx111_get_3d_calib_data\n");
+  return 0;
+}
