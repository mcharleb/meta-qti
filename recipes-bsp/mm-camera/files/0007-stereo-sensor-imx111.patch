+/*===========================================================================
+ * FUNCTION    - qs_imx111_real_to_register_gain -
+ *
+ * DESCRIPTION:
+ *==========================================================================*/
+static uint16_t qs_imx111_real_to_register_gain(float gain)
+{
+  uint16_t reg_gain;
+
+  if (gain < 1.0)
+    gain = 1.0;
+
+  if (gain > 8.0)
+    gain = 8.0;
+
+  reg_gain = (uint16_t)(256.0 - 256.0 / gain);
+
+  return reg_gain;
+}
+
+/*===========================================================================
+ * FUNCTION    - qs_imx111_register_to_real_gain -
+ *
+ * DESCRIPTION:
+ *==========================================================================*/
+float qs_imx111_register_to_real_gain(uint16_t reg_gain)
+{
+  float gain;
+
+  if (reg_gain > 0x00E0)
+    reg_gain = 0x00E0;
+
+  gain = 256.0 /(256.0 - reg_gain);
+
+  return gain;
+}
