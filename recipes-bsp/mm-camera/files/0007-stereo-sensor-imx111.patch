+/*===========================================================================
+ * FUNCTION    - x -
+ *
+ * DESCRIPTION: Chromatix Adjustment
+ *==========================================================================*/
+int8_t qs_imx111_adjust_chromatix(void *ctrl)
+{
+  struct sensor_cfg_data cfg;
+  uint16_t exp_tbl_num_entries = 0, i;
+  sensor_ctrl_t *sctrl = (sensor_ctrl_t *) ctrl;
+  chromatix_parms_type *chromatixptr = &(sctrl->chromatixData);
+
+  CDBG("qs_imx111_sensor_config entering \n");
+
+  if (sctrl->sfd <= 0)
+    return FALSE;
+
+  if (sctrl->sensor.cam_mode == CAMERA_MODE_3D) {
+    qs_imx111_get_3d_calib_data(sctrl);
+    if (qs_imx111_delta.r_delta < 1 ||
+        qs_imx111_delta.b_delta < 1) {
+        min_gain = MIN(qs_imx111_delta.r_delta, qs_imx111_delta.b_delta);
+      exp_tbl_num_entries = chromatixptr->chromatix_exposure_table.valid_entries;
+      for (i = 0; i < exp_tbl_num_entries; i++) {
+        chromatixptr->chromatix_exposure_table.exposure_entries[i].gain /= min_gain;
+      }
+    }
+  }
+  return 0;
+}
